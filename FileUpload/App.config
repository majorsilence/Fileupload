<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7"/>
    </startup>

  <appSettings>
    <!-- FileProvider: sftp, azureblob -->
    <add key="FileProvider" value="sftp"/>

    <!-- sftp settings:-->
    <add key="Host" value="ssh server"/>
    <add key="Port" value="22"/>
    <add key="Username" value=""/>
    <add key="Password" value=""/>

    <!-- azure settings:-->
    <add key="ConnectionString" value="ssh server"/>
    <add key="Container" value=""/>

  </appSettings>
</configuration>
